{
	"name": "@pragmatic-engineering/svelte-form-builder-community",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/pragmatic-engineering/svelte-form-builder-community.git",
		"directory": "@pragmatic-engineering/svelte-form-builder-community"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/pragmatic-engineering"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"sk-publish": "cd package && npm publish",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "next",
		"@types/lodash.merge": "^4.6.7",
		"@types/signature_pad": "^2.3.2",
		"@types/tabulator-tables": "^5.32",
		"@types/tinymce": "^4.6.5",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte": "2.6.0",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.50.1",
		"svelte-check": "^2.9.1",
		"svelte-preprocess": "^4.10.7",
		"svelte-select": "^4.4.7",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4",
		"vite": "^3.1.3"
	},
	"dependencies": {
		"@tinymce/tinymce-svelte": "^1.0.0",
		"@ernane/svelte-star-rating": "^1.1.0",
		"balloon-css": "^1.2.0",
		"lodash.merge": "^4.6.2",
		"nanoid": "^4.0.0",
		"pretty-checkbox": "^3.0.3",
		"signature_pad": "^4.0.8",
		"svelecte": "^3.9.5",
		"svelte-scrollto-element": "^0.7.0",
		"tabulator-tables": "^5.3.4"
	},
	"type": "module"
}
